{
	"name": "WordPress",
	"dockerComposeFile": "./docker-compose.yml",
	"service": "wordpress",
	"workspaceFolder": "/var/www/html/wp-content/themes/ftek-theme",
	"forwardPorts": [8888],
	"extensions": [
		"esbenp.prettier-vscode",
		"wongjn.php-sniffer",
		"csstools.postcss",
		"bradlc.vscode-tailwindcss"
	],
	"remoteUser": "wordpress",
	"onCreateCommand": "sudo -Eu www-data setup-wordpress"
}
